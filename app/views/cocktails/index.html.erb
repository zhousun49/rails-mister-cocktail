<h1 style="text-align: center">Here is the list of Cocktails</h1>
<% @cocktails.each do |c| %>
<p style="text-align: center; margin-top: 2%"><%=link_to c.name, cocktail_path(c) %></p>
<% end %>
<p style="text-align: center; margin-top: 2%"><%= link_to "Create a New Cocktail", new_cocktail_path, class: "btn btn-primary" %></p>
